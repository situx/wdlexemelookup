{
  "manifest_version": 2,

  "name": "Wikidata Lexeme Popup",
  "description": "This extension resolves lexemes from Wikidata     ",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "lookup.html"
  },
    "background":
    {
        "scripts": ["jquery.js", "jquery-ui.min.js"]
    },
    "content_scripts":
    [
        {
            "matches":["https://*/*"],
            "js": ["d3-sparql.min.js","jquery.js", "jquery-ui.min.js","lookup.js"],
            "css": ["style.css"],
            "run_at": "document_end"
        }
    ],
  "permissions": [
   "activeTab"
   ]
}
